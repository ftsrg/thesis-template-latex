    \expandafter\expandafter\expandafter\magyar@fo@pickzero\csname r@footnote&\the\c@footnote@ch&\the\@tempcnta\endcsname00\hfuzz\magyar@fo@@prevfn% previous footnote
    \advance\@tempcnta\@ne
    %
    \ifx\reserved@a\magyar@fo@@prevfn\else
